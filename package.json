    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
